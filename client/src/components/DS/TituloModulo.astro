---
interface Props {
  titulo: string;
  alias:string;
}

const { titulo,alias } = Astro.props as Props;
---

<div class="flex items-center justify-between mx-2 my-5">
  <a href={`/categoria/${alias}`}="" class="bg-green-500  w-16 rounded-br-xl rounded-tr-xl">
    <h2 class="ml-1 font-bold text-2xl">{titulo}</h2>
  </a>
  <a href={`/categoria/${alias}`}><h2 class="text-xs">Ver todos</h2></a>
</div>
